{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "can-work": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist-ssr/functions/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "allowedCommonJsDependencies": [
              "stream",
              "assert",
              "os",
              "crypto",
              "util",
              "http",
              "https"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["src/assets/sass"]
            },
            "optimization": {
              "scripts": true,
              "styles": {
                "minify": true,
                "inlineCritical": false
              },
              "fonts": true
            },
            "outputHashing": "all",
            "sourceMap": true,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false,
            "buildOptimizer": true,
            "assets": ["src/assets", "src/favicon.ico"],
            "styles": [
              "node_modules/firebaseui/dist/firebaseui.css",
              "node_modules/animate.css/animate.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss",
              "node_modules/ngx-spinner/animations/ball-clip-rotate-pulse.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/ngx-modalview/styles/ngx-modal.css",
              "src/assets/massimo/css/primeng.css",
              "src/assets/sass/global.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "staging": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "can-work:build"
          },
          "configurations": {
            "staging": {
              "browserTarget": "can-work:build:staging"
            },
            "prod": {
              "browserTarget": "can-work:build:prod"
            },
            "production": {
              "browserTarget": "can-work:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "can-work:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "node_modules/firebaseui/dist/firebaseui.css",
              "node_modules/animate.css/animate.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss",
              "node_modules/ngx-spinner/animations/ball-clip-rotate-pulse.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/ngx-modalview/styles/ngx-modal.css",
              "src/assets/massimo/css/primeng.css",
              "src/assets/sass/global.scss"
            ],
            "assets": ["src/assets", "src/favicon.ico"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist-ssr/functions/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "buildOptimizer": true,
            "outputHashing": "media",
            "optimization": true,
            "sourceMap": true,
            "stylePreprocessorOptions": {
              "includePaths": ["src/assets/sass"]
            },
            "extractLicenses": true,
            "vendorChunk": false
          },
          "configurations": {
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "buildOptimizer": true,
              "outputHashing": "media",
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "can-work:build:development",
              "serverTarget": "can-work:server:development"
            },
            "production": {
              "browserTarget": "can-work:build:production",
              "serverTarget": "can-work:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "can-work:build:production",
              "serverTarget": "can-work:server:production"
            },
            "development": {
              "browserTarget": "can-work:build:development",
              "serverTarget": "can-work:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "can-work-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "can-work:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "analytics": false
  }
}
