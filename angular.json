{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "can-work": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "allowedCommonJsDependencies": [
              "stream",
              "assert",
              "os",
              "crypto",
              "util",
              "http",
              "https"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["src/assets/sass"]
            },
            "optimization": {
              "scripts": true,
              "styles": {
                "minify": true,
                "inlineCritical": false
              },
              "fonts": false
            },
            "outputHashing": "all",
            "sourceMap": true,
            "aot": true,
            "namedChunks": true,
            "extractLicenses": false,
            "vendorChunk": true,
            "buildOptimizer": false,
            "assets": ["src/assets", "src/favicon.ico"],
            "styles": [
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "node_modules/firebaseui/dist/firebaseui.css",
              "node_modules/animate.css/animate.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss",
              "src/assets/sass/global.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "staging": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "can-work:build"
          },
          "configurations": {
            "staging": {
              "browserTarget": "can-work:build:staging"
            },
            "prod": {
              "browserTarget": "can-work:build:prod"
            },
            "production": {
              "browserTarget": "can-work:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "can-work:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "node_modules/firebaseui/dist/firebaseui.css",
              "node_modules/animate.css/animate.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss",
              "src/assets/sass/global.scss"
            ],
            "assets": ["src/assets", "src/favicon.ico"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "can-work-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "can-work:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "can-work",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
