    "angular-instantsearch": "^4.4.0",
    "angular-star-rating": "^6.0.0",
    "angular2-qrcode": "^2.0.1",
    "angularfire2": "^5.2.0",
    "animate.css": "^3.6.1",
    "base64-js": "^1.3.1",
    "core-js": "^2.4.1",
    "ethers": "5.3.1",
